    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "backend",
            "request": "launch",
            "type": "dart",
            "program": "backend/bin/backend.dart"
        },
        {
            "name": "front",
            "cwd": "front",
            "request": "launch",
            "type": "dart",
            "program": "front/lib/main.dart",
            "toolArgs": [
                "--dart-define",
                "HOST=localhost:5555",
            ]
        },
        {
            "name": "front (profile mode)",
            "cwd": "front",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "front/lib/main.dart",
            "toolArgs": [
                "--dart-define",
                "HOST=localhost:5555",
            ]
        },
        {
            "name": "front (release mode)",
            "cwd": "front",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "front/lib/main.dart",
            "toolArgs": [
                "--dart-define",
                "HOST=localhost:5555",
            ]
        }
    ]
}